#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
def init
  welcome
  board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
  display_board(default_board)
  puts "Where do you want to move?"
  input = gets.strip
  index = input_to_index(input)
  move(board, index)
end

init
